{
  "expo": {
    "name": "Portal",
    "slug": "portal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "portal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a365d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "systems.kaia.portal",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "Find tmux agents on your local network."
      }
    },
    "android": {
      "package": "systems.kaia.portal",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a365d"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-live-activity"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "LiveActivity",
                  "bundleIdentifier": "systems.kaia.portal.LiveActivity",
                  "entitlements": {}
                }
              ]
            }
          }
        },
        "projectId": "6d09bd37-08b3-4f00-929e-a0cca565fd1d"
      }
    },
    "owner": "kaiasystems"
  }
}
